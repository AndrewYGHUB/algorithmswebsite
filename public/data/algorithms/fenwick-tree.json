      "code": "<span class=\"hljs-comment\">/*\n * Author: Mohit Kumar\n * Fedwick Tree Implementation in JavaScript\n * Fedwick Tree Implementation for finding prefix sum.\n*/</span>\n\n<span class=\"hljs-keyword\">class</span> <span class=\"hljs-title class_\">FenwickTree</span> {\n  <span class=\"hljs-title function_\">constructor</span> (feneickArray, array, n) {\n    <span class=\"hljs-keyword\">for</span> (<span class=\"hljs-keyword\">let</span> i = <span class=\"hljs-number\">1</span>; i &lt;= n; i++) {\n      feneickArray[i] = <span class=\"hljs-number\">0</span>\n    }\n    <span class=\"hljs-keyword\">for</span> (<span class=\"hljs-keyword\">let</span> i = <span class=\"hljs-number\">0</span>; i &lt; n; i++) {\n      <span class=\"hljs-variable language_\">this</span>.<span class=\"hljs-title function_\">update</span>(feneickArray, n, i, array[i])\n    }\n  }\n\n  update (feneickArray, n, index, value) {\n    index = index + <span class=\"hljs-number\">1</span>\n    <span class=\"hljs-keyword\">while</span> (index &lt;= n) {\n      feneickArray[index] += value\n      index += index &amp; (-index)\n    }\n  }\n\n  getPrefixSum (feneickArray, index) {\n    <span class=\"hljs-keyword\">let</span> currSum = <span class=\"hljs-number\">0</span>\n    index = index + <span class=\"hljs-number\">1</span>\n    <span class=\"hljs-keyword\">while</span> (index &gt; <span class=\"hljs-number\">0</span>) {\n      currSum += feneickArray[index]\n      index -= index &amp; (-index)\n    }\n\n    <span class=\"hljs-keyword\">return</span> currSum\n  }\n}\n<span class=\"hljs-keyword\">export</span> { <span class=\"hljs-title class_\">FenwickTree</span> }\n"
